/*2018.03.14*/
.padding { padding: 20rpx; }
.padding30 { padding: 30rpx; }
.padding-top { padding-top: 20rpx; }
.margin-top { margin-top: 20rpx; }
.border-top { border-top: 1px solid #eee; }
.border-bottom { border-bottom: 1px solid #eee; }
.text-right { text-align: right; }
.bg-white { background-color: #ffffff; }
.overflow-hidden { overflow: hidden; }



.wsui-cell {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
}
.wsui-cell__hd {
    font-size: 1.1em;
}
.wsui-cell__ft {
    display: flex;
    align-items: center;
    font-size: .9em;
    color: #999;
}
.wsui-cell__ft-arrow {
     width: 1em;
     height: 1em;
     background-image: url("data:image/png;data:image/png;base64,");
     background-repeat: no-repeat;
     background-size: cover;
     margin-left: .5em;
}

.wsui-btn {
    border-radius: 4px;
    padding: .4em;
    color: #fff;
}
.wsui-btn-icon {
    display: flex;
    align-items: center;
}
.wsui-btn-icon__icon {
    width: 1em;
    height: 1em;
    margin-right: .5em;
}

.wsui-bar_btn {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 100%;
    width: 100%;
    background-color: #EC4149;
}

.wsui-ft-btn {
    display: flex;
    justify-content: center;
    align-items: center;
    color: #fff;
    border-radius: 4px;
    width: 95%;
    height: 3em;
    margin-left: auto;
    margin-right: auto;
}
.wsui-ft-btn--warning {
    background-color: #FA1637;
}